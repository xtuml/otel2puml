@startuml
partition "simple_XOR_job" {
    group "simple_XOR_job"
        #green:A;
        :B;
        switch (XOR)
            case ("1")
                :C;
            case ("2")
                :D;
            case ("3")
                :E;
        endswitch
        #red:F;
    end group
}
@enduml